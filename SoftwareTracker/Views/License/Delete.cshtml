@model SoftwareTracker.Models.LicenseModel

@{
    ViewData["Title"] = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div>
    <h3 class="jet-color">Are you sure you want to delete this?</h3>
    <div class="details-container">
        <dl class="row">
            <dt class="col-sm-2 jet-color">Manufacturer</dt>
            <dd class="col-sm-10">@Html.DisplayFor(model => model.Manufacturer)</dd>
            <dt class="col-sm-2 jet-color">Software Title</dt>
            <dd class="col-sm-10">@Html.DisplayFor(model => model.SoftwareTitle)</dd>
            <dt class="col-sm-2 jet-color">Assigned Server</dt>
            <dd class="col-sm-10">@Html.DisplayFor(model => model.AssignedServer)</dd>
            <dt class="col-sm-2 jet-color">Purchase Order</dt>
            <dd class="col-sm-10">@Html.DisplayFor(model => model.PurchaseOrder)</dd>
            <dt class="col-sm-2 jet-color">Purchase Date</dt>
            <dd class="col-sm-10">@Html.DisplayFor(model => model.PurchaseDate)</dd>
            <dt class="col-sm-2 jet-color">License Type</dt>
            <dd class="col-sm-10">@Html.DisplayFor(model => model.LicenseType)</dd>
            <dt class="col-sm-2 jet-color">License Expiry</dt>
            <dd class="col-sm-10">@Html.DisplayFor(model => model.LicenseExp)</dd>
            <dt class="col-sm-2 jet-color">Support</dt>
            <dd class="col-sm-10">@Html.DisplayFor(model => model.Support)</dd>
            <dt class="col-sm-2 jet-color">Support Expiry</dt>
            <dd class="col-sm-10">@Html.DisplayFor(model => model.SupportExp)</dd>
            <dt class="col-sm-2 jet-color">Amount of Keys</dt>
            <dd class="col-sm-10">@Html.DisplayFor(model => model.AmountofKeys)</dd>
            <dt class="col-sm-2 jet-color">Used Keys</dt>
            <dd class="col-sm-10">@Html.DisplayFor(model => model.UsedKeys)</dd>
            <dt class="col-sm-2 jet-color">Remaining Keys</dt>
            <dd class="col-sm-10">@Html.DisplayFor(model => model.RemainingKeys)</dd>
            <dt class="col-sm-2 jet-color">License Key</dt>
            <dd class="col-sm-10">@Html.DisplayFor(model => model.LicenseKey)</dd>
        </dl>
    </div>
    <div>
        <form asp-action="Delete">
            <input type="hidden" asp-for="Id" />
            <input type="submit" value="Delete" class="btn btn-danger" /> |
            <a class="btn btn-primary" asp-action="Index">Back to List</a>
        </form>
    </div>
</div>
